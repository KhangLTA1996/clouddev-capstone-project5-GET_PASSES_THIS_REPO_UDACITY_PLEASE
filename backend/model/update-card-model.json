{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "UpdateCard",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 200
    },
    "description": {
      "type": "string"
    },
    "dueDate": {
      "type": "string"
    },
    "completed": {
      "type": "boolean"
    },
    "attachmentUrl": {
      "type": "string"
    }
  },
  "required": ["title", "dueDate", "completed", "attachmentUrl"],
  "additionalProperties": false
}
